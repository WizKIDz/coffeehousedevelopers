<p id="notice"><%= notice %></p>

<h1>Listing Projects</h1>
<% @projects.each do |project| %>
<div class="project">
  <%= image_tag project.picture.url(:medium) , :class => 'project-image'%>
  <div class="content">
    <h3 class="text-center"><%= project.title %></h3>
  </div>
</div>
<% end %>
<br>
